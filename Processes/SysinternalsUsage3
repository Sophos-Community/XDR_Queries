-- DEFINE THE FOLLOWING VARIABLE:
--$$Tool$$

select * from sophos_process_journal as spj join sophos_registry_journal as srj on spj.sophospid=srj.sophospid where srj.keyname like '%\SOFTWARE\Sysinternals\$$Tool$$')
